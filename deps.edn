{:deps {io.github.clojure/tools.build {:mvn/version "0.9.4"}}
 :paths ["src"]
 
 :aliases
 {:jar
  {:exec-fn monkey.build/jar
   :exec-args {:jar "target/monkey-build.jar"}}

  :install
  {:exec-fn monkey.build/install
   :exec-args {:jar "target/monkey-build.jar"
               :version "0.1.0-SNAPSHOT"
               :lib "monkey/build"}}}}
